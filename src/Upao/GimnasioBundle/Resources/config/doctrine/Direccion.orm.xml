<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Upao\GimnasioBundle\Entity\Direccion" table="Direccion">
    <id name="iddireccion" type="decimal" column="IdDireccion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tipo" type="string" column="Tipo" length="10" nullable="false"/>
    <field name="calle" type="string" column="Calle" length="150" nullable="false"/>
    <field name="numero" type="string" column="Numero" length="50" nullable="true"/>
    <field name="referencia" type="string" column="Referencia" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
